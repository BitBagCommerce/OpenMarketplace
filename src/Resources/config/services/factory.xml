<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://symfony.com/schema/dic/services"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="false" autoconfigure="false" />

        <service class="BitBag\SyliusMultiVendorMarketplacePlugin\Factory\Message\MessageFactory"
                 id="bitbag.sylius_multi_vendor_marketplace_plugin.factory.message.message_factory"
                 decorates="bitbag_multi_vendor_marketplace.factory.conversation_message" >
            <argument type="service" id="bitbag.sylius_multi_vendor_marketplace_plugin.factory.message.message_factory.inner" />

        </service>
        <service class="BitBag\SyliusMultiVendorMarketplacePlugin\Factory\VendorProfileFactory"
                 id="bitbag_mvm_plugin.factory.vendor_profile_factory"
                 public="true">
        </service>

        <service class="BitBag\SyliusMultiVendorMarketplacePlugin\Factory\AddressFactory"
                 id="bitbag_mvm_plugin.factory.vendor_address_factory"
                 public="true">
        </service>

        <service class="BitBag\SyliusMultiVendorMarketplacePlugin\Factory\VendorProfileUpdateFactory"
                 id="bitbag_mvm_plugin.factory.vendor_profile_update_factory">
            <argument type="service" id="bitbag_mvm_plugin.generator.token_generator"/>
        </service>
        <service class="BitBag\SyliusMultiVendorMarketplacePlugin\Factory\AddressFactory" id="bitbag.mvm.factory.address_factory">

        </service>
    </services>
</container>
