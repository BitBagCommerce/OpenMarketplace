<?xml version="1.0" encoding="utf-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="bitbag.command.product_listing.create" class="BitBag\SyliusMultiVendorMarketplacePlugin\Command\ProductListing\CreateProductListingCommand">
            <argument type="service" id="bitbag_mvm_plugin.repository.product_listing"/>
            <argument type="service" id="bitbag_mvm_plugin.factory.product_listing" />
            <argument type="service" id="security.token_storage" />
            <argument type="service" id="bitbag_mvm_plugin.factory.product_translation" />
            <argument type="service" id="bitbag_mvm_plugin.factory.product_draft" />
            <argument type="service" id="bitbag_mvm_plugin.factory.product_listing_price" />
            <argument type="service" id="bitbag_mvm_plugin.repository.product_draft"/>
            <argument type="service" id="bitbag_mvm_plugin.state_machine.product_draft.transition"/>
        </service>
    </services>
</container>
