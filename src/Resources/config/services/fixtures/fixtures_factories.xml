<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true" />

        <service id="bitbag.mvm_plugin.fixture.example_factory.vendor" class="BitBag\SyliusMultiVendorMarketplacePlugin\Fixture\Factory\VendorExampleFactory">
            <argument type="service" id="bitbag_mvm_plugin.factory.vendor_profile_factory" />
            <argument type="service" id="bitbag_mvm_plugin.factory.vendor_address_factory" />
            <argument type="service" id="sylius.factory.shop_user" />
            <argument type="service" id="sylius.factory.customer" />
            <argument type="service" id="sylius.repository.country" />
            <argument type="service" id="sylius.repository.customer_group" />
        </service>

        <service id="bitbag.mvm_plugin.fixture.example_factory.product_draft" class="BitBag\SyliusMultiVendorMarketplacePlugin\Fixture\Factory\ProductDraftExampleFactory">
            <argument type="service" id="bitbag_mvm_plugin.factory.product_draft" />
            <argument type="service" id="bitbag_mvm_plugin.factory.product_listing_price" />
            <argument type="service" id="bitbag_mvm_plugin.factory.product_listing_from_draft_factory" />
            <argument type="service" id="bitbag_mvm_plugin.factory.product_translation" />
            <argument type="service" id="bitbag_mvm_plugin.repository.vendor" />
            <argument type="service" id="sylius.repository.channel" />
            <argument type="service" id="sylius.repository.locale" />
            <argument type="service" id="bitbag_mvm_plugin.state_machine.product_draft.transition" />
            <argument type="service" id="sylius.generator.slug" />
        </service>

        <service id="bitbag.mvm_plugin.fixture.example_factory.order" class="BitBag\SyliusMultiVendorMarketplacePlugin\Fixture\Factory\OrderExampleFactory">
            <argument type="service" id="bitbag.mvm_plugin.factory.order_factory" />
            <argument type="service" id="bitbag.mvm_plugin.factory.order_item_factory" />
            <argument type="service" id="sylius.order_item_quantity_modifier" />
            <argument type="service" id="sylius.manager.order" />
            <argument type="service" id="sylius.repository.channel" />
            <argument type="service" id="sylius.repository.customer" />
            <argument type="service" id="sylius.repository.product" />
            <argument type="service" id="sylius.repository.country" />
            <argument type="service" id="sylius.repository.payment_method" />
            <argument type="service" id="sylius.repository.shipping_method" />
            <argument type="service" id="sylius.factory.address" />
            <argument type="service" id="sm.factory" />
            <argument type="service" id="bitbag.mvm_plugin.processor.order.split_order_by_vendor_processor"/>
        </service>

        <service id="bitbag.mvm_plugin.fixture.example_factory.conversation" class="BitBag\SyliusMultiVendorMarketplacePlugin\Fixture\Factory\ConversationExampleFactory">
            <argument type="service" id="bitbag_mvm_plugin.factory.conversation" />
            <argument type="service" id="bitbag_multi_vendor_marketplace.factory.conversation_message" />
            <argument type="service" id="bitbag.mvm_plugin.repository.vendor_repository" />
            <argument type="service" id="sylius.repository.admin_user" />
            <argument type="service" id="bitbag_mvm_plugin.repository.conversation_category" />
        </service>
    </services>
</container>
