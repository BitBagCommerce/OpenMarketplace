<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <defaults public="true" />

        <service id="BitBag\OpenMarketplace\Api\Messenger\CommandHandler\Vendor\RegisterVendorHandler">
            <argument type="service" id="open_marketplace.api.provider.vendor" />
            <argument type="service" id="doctrine.orm.default_entity_manager" />
            <tag name="messenger.message_handler" bus="sylius.command_bus" />
            <tag name="messenger.message_handler" bus="sylius_default.bus" />
        </service>
    </services>
